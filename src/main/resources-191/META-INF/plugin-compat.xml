<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.fileType" beanClass="uk.co.reecedunn.compat.fileTypes.FileTypeBean">
      <with attribute="implementationClass" implements="com.intellij.openapi.fileTypes.FileType"/>
    </extensionPoint>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="uk.co.reecedunn.compat.fileTypes.FileTypeFactory"/>
  </extensions>
</idea-plugin>
