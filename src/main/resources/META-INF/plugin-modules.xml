<idea-plugin>
  <extensionPoints>
    <!--suppress PluginXmlValidity -->
    <extensionPoint qualifiedName="uk.co.reecedunn.intellij.importPathResolver" interface="uk.co.reecedunn.intellij.plugin.xpm.module.ImportPathResolver" dynamic="true"/>
    <!--suppress PluginXmlValidity -->
    <extensionPoint qualifiedName="uk.co.reecedunn.intellij.modulePathFactory" interface="uk.co.reecedunn.intellij.plugin.xdm.module.path.XdmModulePathFactory" dynamic="true"/>
    <!--suppress PluginXmlValidity -->
    <extensionPoint qualifiedName="uk.co.reecedunn.intellij.moduleLoaderFactory" beanClass="uk.co.reecedunn.intellij.plugin.xdm.module.loader.XdmModuleLoaderFactoryBean" dynamic="true">
      <with attribute="implementation" implements="uk.co.reecedunn.intellij.plugin.xdm.module.loader.XdmModuleLoaderFactory"/>
    </extensionPoint>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <projectService serviceImplementation="uk.co.reecedunn.intellij.plugin.xdm.java.JavaTypePath"/>
    <projectService serviceImplementation="uk.co.reecedunn.intellij.plugin.xdm.module.loader.XdmModuleLoaderSettings"/>
  </extensions>

  <extensions defaultExtensionNs="uk.co.reecedunn.intellij">
    <!-- Module Paths -->

    <modulePathFactory implementation="uk.co.reecedunn.intellij.plugin.xdm.module.path.XdmReverseDomainNameModulePath"/>
    <modulePathFactory implementation="uk.co.reecedunn.intellij.plugin.xdm.module.path.XdmModuleLocationPath$Companion"/>
    <modulePathFactory implementation="uk.co.reecedunn.intellij.plugin.xdm.java.JavaModulePath$Companion"/>
    <modulePathFactory implementation="uk.co.reecedunn.intellij.plugin.xdm.java.JavaTypePath$Companion"/>
    <modulePathFactory implementation="uk.co.reecedunn.intellij.plugin.xdm.module.path.XdmModuleUriPath"/>

    <!-- Module Loaders -->

    <moduleLoaderFactory name="fixed" implementation="uk.co.reecedunn.intellij.plugin.xpm.module.loader.impl.FixedModuleLoader$Companion"/>
    <moduleLoaderFactory name="jar" implementation="uk.co.reecedunn.intellij.plugin.xpm.module.loader.impl.JarModuleLoader$Companion"/>
    <moduleLoaderFactory name="java" implementation="uk.co.reecedunn.intellij.plugin.xdm.java.JavaModuleLoader"/>
    <moduleLoaderFactory name="module" implementation="uk.co.reecedunn.intellij.plugin.xpm.module.loader.impl.JspModuleSourceRootLoader$Companion"/>
    <moduleLoaderFactory name="package" implementation="uk.co.reecedunn.intellij.plugin.expath.pkg.EXPathPackage$Companion"/>
    <moduleLoaderFactory name="relative" implementation="uk.co.reecedunn.intellij.plugin.xpm.module.loader.impl.RelativeModuleLoader"/>

    <!-- Import Paths : Deprecated by Module Paths -->

    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.xquery.model.Annotations"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.expath.model.BuiltInFunctions"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.exquery.model.BuiltInFunctions"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.w3.model.BuiltInFunctions"/>

    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.basex.model.BuiltInFunctions"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.existdb.model.BuiltInFunctions"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.marklogic.model.BuiltInFunctions"/>
    <importPathResolver implementation="uk.co.reecedunn.intellij.plugin.saxon.model.BuiltInFunctions"/>
  </extensions>
</idea-plugin>
