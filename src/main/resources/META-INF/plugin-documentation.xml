<idea-plugin>
  <extensionPoints>
    <!--suppress PluginXmlValidity -->
    <extensionPoint qualifiedName="uk.co.reecedunn.intellij.documentationSourceProvider" interface="uk.co.reecedunn.intellij.plugin.xqdoc.documentation.XQDocDocumentationSourceProvider" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <applicationService serviceImplementation="uk.co.reecedunn.intellij.plugin.xqdoc.documentation.XdmDocumentationDownloader"/>
  </extensions>

  <extensions defaultExtensionNs="uk.co.reecedunn.intellij">
    <documentationSourceProvider implementation="uk.co.reecedunn.intellij.plugin.w3.documentation.FunctionsAndOperatorsDocumentation"/>
    <documentationSourceProvider implementation="uk.co.reecedunn.intellij.plugin.marklogic.documentation.MarkLogicProductDocumentation"/>
  </extensions>
</idea-plugin>
