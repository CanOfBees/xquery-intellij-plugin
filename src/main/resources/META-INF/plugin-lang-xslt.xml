<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <standardResourceProvider implementation="uk.co.reecedunn.intellij.plugin.xslt.intellij.javaee.XsltResourceProvider"/>
    <standardResourceProvider implementation="uk.co.reecedunn.intellij.plugin.marklogic.intellij.javaee.MarkLogicResourceProvider"/>
    <standardResourceProvider implementation="uk.co.reecedunn.intellij.plugin.saxon.intellij.javaee.SaxonResourceProvider"/>

    <multiHostInjector implementation="uk.co.reecedunn.intellij.plugin.xslt.intellij.lang.injection.XsltSchemaTypeLanguageInjection"/>

    <xmlStructureViewElementProvider implementation="uk.co.reecedunn.intellij.plugin.xpm.intellij.ide.structureView.XmlStructureViewElementProvider" order="last"/>

    <completion.contributor language="XMLPath" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.completion.xpath.XPathCompletionContributor"/>
    <typedHandler implementation="uk.co.reecedunn.intellij.plugin.xpath.intellij.codeInsight.editorActions.XPathTypedHandler"/>

    <xml.nsColorProvider implementation="uk.co.reecedunn.intellij.plugin.xslt.intellij.lexer.XsltNSColorProvider"/>

    <spellchecker.bundledDictionaryProvider implementation="uk.co.reecedunn.intellij.plugin.xslt.intellij.spellchecker.XsltBundledDictionaryProvider"/>

    <!-- XSLT Schema Type -->

    <lang.parserDefinition language="xsl:avt" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslAVT$ParserDefinition"/>
    <lang.parserDefinition language="xsl:EQName" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslEQName$ParserDefinition"/>
    <lang.parserDefinition language="xsl:item-type" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslItemType$ParserDefinition"/>
    <lang.parserDefinition language="xsl:prefixes" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslPrefixes$ParserDefinition"/>
    <lang.parserDefinition language="xsl:QName" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslQName$ParserDefinition"/>
    <lang.parserDefinition language="xsl:QNames" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslQNames$ParserDefinition"/>
    <lang.parserDefinition language="xsl:sequence-type" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.parser.schema.XslSequenceType$ParserDefinition"/>

    <lang.syntaxHighlighterFactory language="xsl:avt" implementationClass="uk.co.reecedunn.intellij.plugin.xslt.intellij.lexer.XsltSyntaxHighlighter$XslAvtFactory"/>
  </extensions>
</idea-plugin>
