<!-- MarkLogic Server-Side JavaScript -->
<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <fileType name="Server-side JavaScript" language="MLJavaScript" extensions="sjs"
              implementationClass="uk.co.reecedunn.intellij.plugin.marklogic.intellij.fileTypes.ServerSideJavaScriptFileType"
              fieldName="INSTANCE"/>
    <fileType name="MarkLogic JavaScript Module" language="MLJavaScriptModule" extensions="mjs"
              implementationClass="uk.co.reecedunn.intellij.plugin.marklogic.intellij.fileTypes.JavaScriptModuleFileType"
              fieldName="INSTANCE"/>
  </extensions>
</idea-plugin>
